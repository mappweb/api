{
    "name": "mappweb/api",
    "description": "Api utilities for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Diego",
            "email": "diego.toscanof@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/passport": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Mappweb\\Api\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mappweb\\Api\\ApiServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-protocols": [
            "https,ssh"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    }
}
